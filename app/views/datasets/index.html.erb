<h1>Listing datasets</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Html</th>
      <th>Mi url</th>
      <th>Bloque</th>
      <th>Campo1</th>
      <th>Campo2</th>
      <th>Campo3</th>
      <th>Campo4</th>
      <th>Campo5</th>
      <th>Campo6</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datasets.each do |dataset| %>
      <tr>
        <td><%= dataset.nombre %></td>
        <td><%= dataset.html %></td>
        <td><%= dataset.mi_url %></td>
        <td><%= dataset.bloque %></td>
        <td><%= dataset.campo1 %></td>
        <td><%= dataset.campo2 %></td>
        <td><%= dataset.campo3 %></td>
        <td><%= dataset.campo4 %></td>
        <td><%= dataset.campo5 %></td>
        <td><%= dataset.campo6 %></td>
        <td><%= link_to 'Show', dataset %></td>
        <td><%= link_to 'Edit', edit_dataset_path(dataset) %></td>
        <td><%= link_to 'Destroy', dataset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dataset', new_dataset_path %>
